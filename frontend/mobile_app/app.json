{
  "expo": {
    "name": "Legatura",
    "slug": "legatura-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "54.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "userInterfaceStyle": "light",
    "statusBar": {
      "hidden": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.legatura.mobile",
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro"
    }
  }
}